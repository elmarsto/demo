doctype xml
- var current_date = new Date();
- var current_date_str = current_date
urlset(xmlns="http://www.sitemaps.org/schemas/sitemap/0.9")
   url
      loc= locals.base
   for n in locals.sitemap
     url
        loc= locals.base + n.href
   for n in locals.darkmap
     url
        loc= locals.base + n.href
   for article in env.helpers.getArticles(contents)
     url
       loc= locals.base + article.url
