block append header
  include ie.html
  nav.all-100
    ul
      li.logo
        a(href='/')
          img(src='raster/logo.png',alt='Company Name')
      include nav-sitemap
      include nav-extmap
  img.all-100(src='/raster/dummy.png',alt='spinner')

block append footer
  div#pages.all-25.small-100
    include footer-pages
  div#twitter.all-50.small-100
    include footer-twitter
  div#newsletter.all-25.small-100
    include footer-newsletter

block append page
  div#copyright
    include copyright

extends stock

